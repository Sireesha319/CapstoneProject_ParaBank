              "\r",
              "    });\r",
              "});\r",
              "\r",
              "\r",
              "const schema = {\r",
              "\r",
              "  \"type\": \"array\",\r",
              "  \"properties\": {\r",
              "    \"id\": {\r",
              "      \"type\": \"integer\"\r",
              "    },\r",
              "    \"accountId\": {\r",
              "      \"type\": \"integer\"\r",
              "    },\r",
              "    \"type\": {\r",
              "      \"type\": \"string\"\r",
              "    },\r",
              "    \"date\": {\r",
              "      \"type\": \"integer\"\r",
              "    },\r",
              "    \"amount\": {\r",
              "      \"type\": \"number\"\r",
              "    },\r",
              "    \"description\": {\r",
              "      \"type\": \"string\"\r",
              "    }\r",
              "  },\r",
              "  \"required\": [\r",
              "    \"id\",\r",
              "    \"accountId\",\r",
              "    \"type\",\r",
              "    \"date\",\r",
              "    \"amount\",\r",
              "    \"description\"\r",
              "  ]\r",
              "}\r",
              "\r",
              "pm.test(\"Schema is valid\", () => {\r",
              "    pm.expect(jsonData).to.have.jsonSchema(schema);\r",
              "});\r",
              "\r",
              "pm.test(\"All transactions have the same accountId\", () => {\r",
              "\r",
              "    // take the first accountId from the response\r",
              "    const accountId = jsonData[0].accountId;\r",
              "\r",
              "    // loop through all items\r",
              "    jsonData.forEach(item => {\r",
              "        pm.expect(item.accountId).to.equal(accountId);\r",
              "    });\r",
              "\r",
              "});\r",
              "\r",
              "\r",
              "pm.test(\"Date must be a valid numeric timestamp\", () => {\r",
              "    pm.expect(jsonData).to.be.an(\"array\").that.is.not.empty;\r",
