    {
      "name": "GET Customer Details (invalid)",
      "event": [
        {
          "listen": "prerequest",
          "script": {
            "exec": [
              "// List of invalid IDs for negative testing\r",
              "const invalidIds = [\"0\", \"-1\", \"abc\", \"!@#\", \"99999999\"];\r",
              "\r",
              "// Store in environment variable for use in Tests tab\r",
              "pm.environment.set(\"invalidIds\", JSON.stringify(invalidIds));\r",
              "\r",
              "console.log(\"Invalid IDs set in environment:\", invalidIds);\r",
              ""
            ],
            "type": "text/javascript",
            "packages": {},
            "requests": {}
          }
        },
        {
          "listen": "test",
          "script": {
            "exec": [
              "// Load invalid IDs from environment\r",
              "const ids = JSON.parse(pm.environment.get(\"invalidIds\"));\r",
              "\r",
              "const baseUrl = pm.environment.get(\"base_url\")\r",
              "\r",
              "pm.test(\"Running negative tests for invalid customer IDs\", () => {\r",
              "    console.log(\"Executing negative tests...\");\r",
              "});\r",
              "\r",
              "// Loop through all invalid IDs\r",
              "ids.forEach(id => {\r",
              "\r",
              "    pm.sendRequest({\r",
              "        url: `${baseUrl}/services/bank/customers/${id}`,\r",
              "        method: \"GET\",\r",
              "        header: { \"Accept\": \"application/json\" }\r",
              "    }, (err, res) => {\r",
              "\r",
              "        console.log(`Testing invalid ID: ${id}, Status: ${res.code}`);\r",
              "\r",
              "        // Should not return OK\r",
              "        pm.test(`Invalid ID ${id} should NOT return 200`, () => {\r",
              "            pm.expect(res.code).not.to.eql(200);\r",
              "        });\r",
              "\r",
              "        // Should return an error code\r",
              "        pm.test(`Invalid ID ${id} returns error`, () => {\r",
              "            pm.expect([400, 404, 405, 500]).to.include(res.code);\r",
              "        });\r",
              "\r",
              "    });\r",
              "\r",
              "});\r",
              ""
            ],
            "type": "text/javascript",
            "packages": {},
            "requests": {}
          }
        }
      ],
